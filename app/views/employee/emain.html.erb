<div class = "wrapper">
 <% if emp_signed_in?%>
 <div class="container-fluid">
    <% if current_emp.sign_in_count == 1 %>
    <div class = "alert alert-success">
           Welcome <%= current_emp.username %>,Hope you will enjoy using this. 
    </div> 
    <%= render "content" %>
      <% else %>
    <div class = "alert alert-success">
           Welcome back <%= current_emp.username %>, You have signed in <%= current_emp.sign_in_count %> times.
    </div>  
    
    <%= render "content" %>

    <%end %>
 </div>

<% else %>
<div class = "alert alert-danger">
    you can not have any authentication to this page 
</div>
<% end %> 
</div>


    

    
      
