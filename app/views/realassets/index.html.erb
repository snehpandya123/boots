<div class="container">
    <div class="row">
        <div class="col-md-9">
            <form class = "form-inline" action="#" method="get">
                <div class="input-group">
           <input type="search" id="search" value="" class="form-control" placeholder="Search using Fuzzy searching">
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                    </span>
                </div>
            </form>
             
             <%= paginate @realasset %>
        </div>
        <div class="col-md-9">
         <table class="table table-striped table-bordered table-condensed" id="table">
                    <thead>
							<tr style = 'background-color: white'>
								<th> <i class="fa fa-male"></i> ID </th>
								<th><i class="fa fa-bars"></i>  Asset name</th>
								<th><i class="fa fa-envelope-o"></i> Cost</th>
								<th><i class="fa fa-map-marker"></i> Type</th>
								<th><i class="fa fa-phone"></i> Location</th>
								<th><i class="fa fa-phone"></i>  TAKE actions</th>
								
							
							</tr>
	</thead>
	<tbody>

      
       <% @realasset.each do |r| %>
      
      <tr>
        <td><%= r.id %></td>
        <td><%= r.name %></td>
        <td><%= r.cost %></td>
        <td><%= r.typ %></td>
       <td> <%= r.locat %></td>
       <td><%= button_to "Remove", r, method: :delete, :class => "btn btn-danger" ,data: { confirm: "Are you sure?" } %></td>
       <tr>
      
        <% end %>
       
   </tbody>
</table>
<div><table >
                <tr>
                    <td>
                        <%= button_to "ADD NEW" , realassets_new_path,:class=>"btn btn-primary",:method => :get %>
                    </td>
                </tr>
            </table>
            
            
        </div>
</div>
</div>
</div>


